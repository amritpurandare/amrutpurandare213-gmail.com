<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main2Activity">

    <com.amrit.circulargraphview.CircularGraph
        android:id="@+id/inner_circle"
        android:layout_width="22dp"
        android:layout_height="22dp"
        app:circle_default_width="4dp"
        app:circle_filled_color="#0000FF"
        app:circle_filled_value="50"
        app:circle_filled_width="4dp"
        app:circle_max_value="100"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <com.amrit.circulargraphview.CircularGraph
        android:id="@+id/outer_circle"
        android:layout_width="36dp"
        android:layout_height="36dp"
        app:circle_default_width="4dp"
        app:circle_filled_color="#00FF00"
        app:circle_filled_value="50"
        app:circle_filled_width="4dp"
        app:circle_max_value="100"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>