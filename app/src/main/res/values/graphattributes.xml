<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircularGraph">

        <!-- maximum value of the default space -->
        <attr name="circle_max_limit" format="integer" />
        <!-- used value out of max value -->
        <attr name="circle_used_limit" format="integer" />

        <!-- circle's default circumference color -->
        <attr name="circle_default_color" format="color" />
        <!-- circle's filled circumference color -->
        <attr name="circle_filled_color" format="color" />

        <!-- circle's default circumference width -->
        <attr name="circle_default_width" format="dimension" />
        <!-- circle's filled circumference width -->
        <attr name="circle_filled_width" format="dimension" />
    </declare-styleable>

</resources>